{
    "env": {
        "includePaths": [
            "${workspaceFolder}/Includes/",
            "${workspaceFolder}/Includes/LibC/"
        ]
    },
    "configurations": [
        {
            "name": "TheOS",
            "intelliSenseMode": "gcc-x86",
            "includePath": [
                "${includePaths}"
            ],
            "cStandard": "c11",
            "browse": {
                "path": [
                    "${workspaceFolder}"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            }
        },
        {
            "name": "AVR",
            "intelliSenseMode": "${default}",
            "cStandard": "c11",
            "cppStandard": "c++14",
            "compilerPath": "",
            "includePath": [],
            "compilerArgs": []
        }
    ],
    "version": 4
}